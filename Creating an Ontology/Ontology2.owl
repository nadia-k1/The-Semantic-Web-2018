  <rdf:Description rdf:about="http://dbpedia.org/resource/Common_Threads:_Stories_from_the_Quilt">
    <ns2:producedBy rdf:resource="http://dbpedia.org/resource/Bill_Couturié"/>
    <ns2:producedBy rdf:resource="http://dbpedia.org/resource/Jeffrey_Friedman_(filmmaker)"/>
