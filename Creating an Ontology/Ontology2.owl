  <rdf:Description rdf:about="http://dbpedia.org/resource/Cocktail_(2012_film)">
    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/Dimple_Kapadia"/>
    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/Saif_Ali_Khan"/>
    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/Diana_Penty"/>
    <ns2:musicBy rdf:resource="http://dbpedia.org/resource/Pritam_Chakraborty"/>
    <ns2:producedBy rdf:resource="http://dbpedia.org/resource/Dinesh_Vijan"/>
