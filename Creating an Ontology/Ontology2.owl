  <rdf:Description rdf:about="http://dbpedia.org/resource/A_Hard_Day's_Night_(film)">
    <ns2:musicBy rdf:resource="http://dbpedia.org/resource/John_Lennon"/>
    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/Wilfrid_Brambell"/>
