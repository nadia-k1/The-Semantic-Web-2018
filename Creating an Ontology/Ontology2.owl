  <rdf:Description rdf:about="http://dbpedia.org/resource/Happiness_(1934_film)">
    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/Pyotr_Zinovyev"/>
    <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/2018/Ontology1.owl#Movie"/>
