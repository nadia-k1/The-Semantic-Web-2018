  <rdf:Description rdf:about="http://dbpedia.org/resource/Gigi_(1958_film)">
    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/Maurice_Chevalier"/>
    <ns2:producedBy rdf:resource="http://dbpedia.org/resource/Arthur_Freed"/>
    <ns2:directedBy rdf:resource="http://dbpedia.org/resource/Vincente_Minnelli"/>
