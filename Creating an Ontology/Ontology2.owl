  <rdf:Description rdf:about="http://dbpedia.org/resource/Saturday_Night_Fever">
    <ns2:movieName xml:lang="en">Saturday Night Fever</ns2:movieName>
    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/Julie_Bovasso"/>
