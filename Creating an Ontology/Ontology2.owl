  <rdf:Description rdf:about="http://dbpedia.org/resource/Swan_Lake_(1981_film)">
    <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/2018/Ontology1.owl#Actor"/>
    <ns2:filmedIn rdf:resource="http://dbpedia.org/resource/Soviet_Union"/>
