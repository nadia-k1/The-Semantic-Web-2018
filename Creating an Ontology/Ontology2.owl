  <rdf:Description rdf:about="http://dbpedia.org/resource/The_Terrorist_(1997_film)">
    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/Ayesha_Dharker"/>
    <ns2:musicBy rdf:resource="http://dbpedia.org/resource/Rajamani"/>
