    <ns2:writtenBy rdf:resource="http://dbpedia.org/resource/Robert_Riskin"/>
    <ns2:directedBy rdf:resource="http://dbpedia.org/resource/Frank_Capra"/>
    <ns2:producedBy rdf:resource="http://dbpedia.org/resource/Frank_Capra"/>
