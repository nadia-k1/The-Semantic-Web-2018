  <rdf:Description rdf:about="http://dbpedia.org/resource/Wings_of_Desire">
    <ns2:writtenBy rdf:resource="http://dbpedia.org/resource/Peter_Handke"/>
    <ns2:directedBy rdf:resource="http://dbpedia.org/resource/Wim_Wenders"/>
    <ns2:musicBy rdf:resource="http://dbpedia.org/resource/Laurent_Petitgand"/>
