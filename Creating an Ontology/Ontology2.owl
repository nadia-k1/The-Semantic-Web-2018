    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/Sheikh_Mukhtar"/>
    <ns2:movieName xml:lang="en">Bahen</ns2:movieName>
    <ns2:filmedIn rdf:resource="http://dbpedia.org/resource/India"/>
