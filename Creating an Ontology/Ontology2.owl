  <rdf:Description rdf:about="http://dbpedia.org/resource/Flash_Gordon_(film)">
    <ns2:directedBy rdf:resource="http://dbpedia.org/resource/Mike_Hodges"/>
    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/Melody_Anderson"/>
    <ns2:writtenBy rdf:resource="http://dbpedia.org/resource/Lorenzo_Semple,_Jr."/>
