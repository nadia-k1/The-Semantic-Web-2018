  <rdf:Description rdf:about="http://dbpedia.org/resource/The_Chronicles_of_Narnia:_The_Voyage_of_the_Dawn_Treader">
    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/Ben_Barnes_(actor)"/>
