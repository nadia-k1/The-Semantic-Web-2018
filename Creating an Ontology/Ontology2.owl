  <rdf:Description rdf:about="http://dbpedia.org/resource/The_Reader_(2008_film)">
    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/Vijessna_Ferkic"/>
    <ns2:hasActor rdf:resource="http://dbpedia.org/resource/David_Kross"/>
